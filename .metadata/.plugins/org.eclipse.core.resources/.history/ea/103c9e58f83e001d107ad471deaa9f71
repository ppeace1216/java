package sec2;

public class Television implements RemoteControl{
	private int volume;

	public int getVolume() {return volume;}
	public void setVolume(int volume) {	this.volume = volume;}

	@Override
	public void turnOn() {
		System.out.println("티비를 켭니다");
	}

	@Override
	public void turnOff() {
		System.out.println("티비를 끕니다");
	}
	
	
}
