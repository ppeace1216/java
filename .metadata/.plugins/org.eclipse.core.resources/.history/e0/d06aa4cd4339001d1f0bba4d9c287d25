package sec2;

public class CarEx {
	public static void main(String[] args) {
		Car car1 = new Car("롤스로이스","가솔린",5000);
		car1.bootUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
		System.out.println("현재속도 : "+car1.speedInfo());	// 값만 나오기 때문에 출력해주는 명령을 적어야 함
		//아예 sysout을 안에 넣었기 때문에 굳이 sysout을 안적어도 됨
		
	}

}
