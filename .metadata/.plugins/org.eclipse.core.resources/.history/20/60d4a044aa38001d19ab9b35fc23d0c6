package sec2;

public class Car {
	String name;	//멤버 필드 : 객체(인스턴스)가 가지는 특성을 저장
	String fuel;
	int cc;
	int speed;
	//[접근제한자] [수정자] 데이터타입 필드명;

	public Car() { }
	public Car(String name){this.name = name;}
	public Car(String name, String fuel){
		this.name = name; this.fuel=fuel;}
	public Car(String name, String fuel, int cc){
		this("peace","eletric");
	}
	public Car(String name, String fuel, int cc, int speed){
		this.name = name; this.fuel = fuel; this.cc=cc; this.speed=speed;
	}
}
