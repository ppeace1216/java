package sec3;

public class Loop8 {

	public static void main(String[] args) {
		// 이중for문을 활용하여 아래와 같은 형태의 구구단을 출력
/*
 * 2*1=2	3*1=3	......
 * 2*2=4	3*2=6	......
 * .......................
 * 2*9=18	
 */
		int [][] a = {{2,1},{3,1},{4,2},{5,3},{6,4},{7,4},{8,1},{9,5}};
		for (int i=2;i<a.length;i++){
			for (int j=1;j<10;j++);{
				System.out.print(a[i][j]+"\t");
			}
			
		}
	}

}
