package sec2;

public class Student1 {
	String name;
	int kor;
	int eng;
	int mat;
	
	public Student1 () {}
	public Student1 (String name){this.name=name;}
	public Student1(String name, int kor, int eng, int mat) {
		this.name = name;
		this.kor = kor;
		this.eng = eng;
		this.mat = mat;
	}
	public int calcTot(){
		return kor+eng+mat;
	}
	public double calcAvg(){
		return kor+eng+mat/3;
	}
	public 
	
}
