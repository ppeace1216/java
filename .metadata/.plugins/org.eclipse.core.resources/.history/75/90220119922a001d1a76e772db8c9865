package sec2;

public class Car1 {
	public static void main(String[] args) {
		int a=1004, b;	//4byte
		short c, d;	//2byte
		long e, f;	//8byte
		b =a;
		//c = a;
		c = 500;
		a = c;	//short -> int = 자동 형변환(auto casting)
		d = (short) b;	// 형 변환 연산자에 의한 형 변환 (force casting)
		System.out.println("d="+d);
		
		
	}

}
