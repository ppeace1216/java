package sec3;

public class Ex2 {

	public static void main(String[] args) {
		int[][] array = {
				{95, 86},
				{83, 92, 96},
				{78, 83, 93, 87, 88}
		};
		int sum = 0;
		double avg =0.0;
		
		for(int c=0;c<array.length;c++);{
			for(int d=0;d<array[c].length;d++){
				sum+=array[c][d];
			}
		}

	}

}
